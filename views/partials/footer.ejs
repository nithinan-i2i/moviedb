<script>
  //Active menu button
  (function() {
    // var nav = document.getElementById('nav'),
    var link = window.document.querySelectorAll('.nav-item .nav-link');
    var active = window.document.getElementsByClassName("selected");
    active[0].className = active[0].className.replace(" selected", "");
    var current = window.location.pathname.split('/', 3)[2];
    for (var i = 0; i < link.length; i++) {
      if (link[i].pathname.split("/", 3)[2] == current) {
        link[i].className += " selected";
      }
    }
  })();
</script>


